<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">


	<servlet>
		<servlet-name>Question</servlet-name>
		<servlet-class>com.kervinramen.spotfinder.facebookapp.controller.QuestionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Question</servlet-name>
		<url-pattern>/question</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Authentication</servlet-name>
		<servlet-class>com.kervinramen.spotfinder.facebookapp.controller.AuthenticationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Authentication</servlet-name>
		<url-pattern>/authenticate</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Helper</servlet-name>
		<servlet-class>com.kervinramen.spotfinder.servlets.HelperServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Helper</servlet-name>
		<url-pattern>/helper</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Kevina</servlet-name>
		<servlet-class>com.kervinramen.spotfinder.servlets.KevinaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Kevina</servlet-name>
		<url-pattern>/kevina</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RemoteApi</servlet-name>
		<servlet-class>com.google.apphosting.utils.remoteapi.RemoteApiServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RemoteApi</servlet-name>
		<url-pattern>/remote_api</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
